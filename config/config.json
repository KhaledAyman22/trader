{
  "telegram_bot_token": "",
  "scan_interval_seconds": 10,
  "historical_lookback_minutes": 1440,
  "chart_resolution": "five_minutes",
  "max_concurrent": 3,
  "api_settings": {
    "headers": {
      "User-Agent": "Mozilla/5.0",
      "Accept": "application/json",
      "Authorization": ""
    },
    "rate_limit_requests_per_minute": 600,
    "request_timeout_seconds": 30
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "database": "trader_db",
    "user": "postgres",
    "password": "admin",
